<h2> All Users </h2>

<div class="table-container">
  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.email %></td>
          <td><%= link_to 'edit', edit_user_path(user) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
